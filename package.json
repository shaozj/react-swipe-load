{
  "name": "react-swipe-load",
  "private": false,
  "version": "0.0.6",
  "description": "react-swipe-load, support down swipe refresh & up swipe load more data",
  "main": "lib/index.js",
  "scripts": {
    "start": "silk server",
    "test": "silk test",
    "compile": "rimraf lib && babel src --out-dir lib && copyfiles -u 1 ./src/**/*.less ./lib",
    "posttest": "npm run lint",
    "build": "silk build",
    "lint": "eslint ./src",
    "clean": "rimraf build/*",
    "fastpush": "npm run compile && git add . && git commit -m 'compile' && git push origin master"
  },
  "repository": "https://github.com/shaozj/react-swipe-load",
  "keywords": [
    "react",
    "swipe",
    "swipe refresh",
    "swipe load"
  ],
  "author": "ShaoZhenjiang<814184517@qq.com>",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-require": "^3.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "copyfiles": "^1.2.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "prop-types": "^15.5.10"
  }
}
